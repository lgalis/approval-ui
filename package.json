    "react-intl": "^2.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-promise-middleware": "^5.1.1",
